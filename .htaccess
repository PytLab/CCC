rewriteEngine On

rewriteRule ^people$ people.php
rewriteRule ^people_details_([0-9]+)$ people_details.php?id=$1
rewriteRule ^people_details_([0-9]+)_([0-9]+)$ people_details.php?id=$1&page=$2
rewriteRule ^people_details_([0-9]+)_([0-9]+)#([a-zA-Z_]+)$ people_details.php?id=$1&page=$2#$3

rewriteRule ^news_list$ news_list.php
rewriteRule ^news_list_([0-9]+)$ news_list.php?page=$1

rewriteRule ^highlights_list$ highlights_list.php
rewriteRule ^highlights_list_([0-9]+)$ highlights_list.php?page=$1

rewriteRule ^publications_list$ publications_list.php
rewriteRule ^publications_list_([0-9]+)$ publications_list.php?page=$1

rewriteRule ^course_list$ course_list.php
rewriteRule ^course_list_([0-9]+)$ course_list.php?page=$1
rewriteRule ^course_login_([0-9]+)$ course_login.php?courseid=$1

rewriteRule ^meeting$ meeting.php
rewriteRule ^meeting([a-zA-Z]+)$ meeting.php?szj=$1

rewriteRule ^contact$ contact.php